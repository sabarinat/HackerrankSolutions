import math
import os
import random
import re
import sys




first_multiple_input = input().rstrip().split()

n = int(first_multiple_input[0])

m = int(first_multiple_input[1])

matrix = []

for _ in range(n):
    matrix_item = input()
    matrix.append(matrix_item)
s=''

for i in zip(*matrix):
    s+=''.join(i)
print(re.sub('(?<=\w)\W+\s*(?=\w)',' ',s))
